<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Activity.StartActivity"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/header"
        android:layout_width="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_height="230dp"
        android:background="@color/colorPrimary"
        android:elevation="8dp">

        <Button
            android:id="@+id/bt_start_config"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/settings"
            android:text=" Configurações"
            android:textColor="#FFFFFF"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:textAllCaps="false"
            android:textSize="18sp"
            style="?android:attr/borderlessButtonStyle"/>

        <ImageButton
            android:id="@+id/ib_start_backspace"
            android:layout_width="80dp"
            android:layout_height="60dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:src="@drawable/backspace"
            style="?android:attr/borderlessButtonStyle"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/tv_start_tmesa"
            android:layout_width="140dp"
            android:layout_height="60dp"
            android:gravity="center_vertical"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            app:autoSizeMaxTextSize="20sp"
            app:autoSizeMinTextSize="14sp"
            android:autoSizeTextType="uniform"
            android:maxLines="1"
            android:lines="1"
            android:includeFontPadding="false"
            android:textStyle="bold"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:textColor="#FFFFFF"
            android:text="COMANDA"/>

        <TextView
            android:id="@+id/tv_start_comanda"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:gravity="center_vertical|center_horizontal"
            android:layout_alignParentBottom="true"
            android:textSize="30sp"
            android:maxLength="6"
            android:layout_toRightOf="@+id/tv_start_tmesa"
            android:layout_toLeftOf="@+id/ib_start_backspace"
            android:textColor="#FFFFFF"
            android:text=""/>

        <View
            android:layout_below="@+id/tv_start_comanda"
            android:layout_width="fill_parent"
            android:layout_height="0.5dp"
            android:layout_marginTop="-11dp"
            android:layout_marginLeft="20dp"
            android:layout_toLeftOf="@+id/ib_start_backspace"
            android:background="#e0ffffff"/>

    </RelativeLayout>

    <RelativeLayout
        app:layout_constraintTop_toBottomOf="@id/header"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="0dp"
        android:layout_height="0dp">

